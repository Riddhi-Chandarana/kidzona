<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main11"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.riddhi.first.Main11Activity"
    android:background="@drawable/xyz">

    <ImageButton
        android:layout_width="190dp"
        android:layout_height="190dp"
        app:srcCompat="@drawable/pink"
        android:id="@+id/pink"
        android:background="@android:color/transparent"
        android:layout_above="@+id/home"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="90dp"
        android:layout_height="90dp"
        app:srcCompat="@drawable/home"
        android:id="@+id/home"
        android:background="@android:color/transparent"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/violet"
        android:id="@+id/violet"
        android:background="@android:color/transparent"
        android:layout_above="@+id/home"
        android:layout_toStartOf="@+id/pink" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/yellow"
        android:id="@+id/yellow"
        android:background="@android:color/transparent"
        android:layout_marginTop="29dp"
        android:layout_alignParentTop="true"
        android:layout_toStartOf="@+id/home" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/green"
        android:id="@+id/green"
        android:background="@android:color/transparent"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/pink" />

</RelativeLayout>
